<h1>Products</h1>

<div class="products">
      <% @products.each do |product| %>
      <div class="data">
        <p><%= product.category %></p>
        <p><%= product.name %></p>
        <p><%= product.size_start %>-<%= product.size_end %></p>
        <p><%= product.price %></p>
      </div>
      <%end%>
</div>


